case \"resolved\": {\n      return {status: \"resolved\", data: action.data, error: null};\n    }\n    case \"rejected\": {\n      return {status: \"rejected\", data: null, error: action.error};\n    }\n    default: {\n      throw new Error(`Unhandled action type: ${action.type}`);\n    }\n  }\n}\n\nconst useAsync = (asyncCallback, initState, dependencies) => {\n  const [state, dispatch] = useReducer(asyncReducer, {\n    status: initState.status || \"idle\",\n    data: initState.data || null,\n    error: initState.error || null,\n  });\n\n  useEffect(\n    () => {\n      const promise = asyncCallback()\n      if (!promise) {\n        return\n      }\n      dispatch({type: \"pending\"});\n      promise.then(\n        data => {\n          dispatch({type: \"resolved\", data});\n        },\n        error => {\n          dispatch({type: \"rejected\", error});\n        },\n      );\n    },\n    [...dependencies],\n  );\n  return state;\n};\n\nfunction PokemonInfo({pokemonName}) {\n  const state = useAsync(\n    () => {\n      if (!pokemonName) {\n        return;\n      }\n      return fetchPokemon(pokemonName);\n    },\n    {status: pokemonName ? \"pending\" : \"idle\"},\n    [pokemonName],\n  );\n\n  const {data, status, error} = state;\n\n  if (status === \"idle\" || !pokemonName) {\n    return \"Submit a pokemon\";\n  } else if (status === \"pending\") {\n    return <PokemonInfoFallback name={pokemonName} />;\n  } else if (status === \"rejected\") {\n    throw error;\n  } else if (status === \"resolved\") {\n    return <PokemonDataView pokemon={data} />;\n  }\n\n  throw new Error(\"This should be impossible\");\n}\n\nfunction App() {\n  const [pokemonName, setPokemonName] = useState(\"\");\n\n  function handleSubmit(newPokemonName) {\n    setPokemonName(newPokemonName);\n  }\n\n  function handleReset() {\n    setPokemonName(\"\");\n  }\n\n  return (\n    <div className=\"pokemon-info-app\">\n      <PokemonForm pokemonName={pokemonName} onSubmit={handleSubmit} />\n      <hr />\n      <div className=\"pokemon-info\">\n        <PokemonErrorBoundary onReset={handleReset} resetKeys={[pokemonName]}>\n          <PokemonInfo pokemonName={pokemonName} />\n        </PokemonErrorBoundary>\n      </div>\n    </div>\n  );\n}\n\nfunction AppWithUnmountCheckbox() {\n  const [mountApp, setMountApp] = useState(true);\n  return (\n    <div>\n      <label>\n        <input\n          type=\"checkbox\"\n          checked={mountApp}\n          onChange={e => setMountApp(e.target.checked)}\n        />{\" \"}\n        Mount Component\n      </label>\n      <hr />\n      {mountApp ? <App /> : null}\n    </div>\n  );\n}\n\nexport default AppWithUnmountCheckbox;\n","/home/david/proxectos/advanced-react-hooks/src/exercise/03.js",[],"/home/david/proxectos/advanced-react-hooks/src/exercise/05.js",[],"/home/david/proxectos/advanced-react-hooks/src/exercise/03.extra-2.js",[],"/home/david/proxectos/advanced-react-hooks/src/exercise/04.js",[],"/home/david/proxectos/advanced-react-hooks/src/exercise/06.js",[],"/home/david/proxectos/advanced-react-hooks/src/final/01.extra-1.js",[],"/home/david/proxectos/advanced-react-hooks/src/final/01.extra-2.js",[],"/home/david/proxectos/advanced-react-hooks/src/final/01.js",[],"/home/david/proxectos/advanced-react-hooks/src/final/01.extra-3.js",[],"/home/david/proxectos/advanced-react-hooks/src/final/01.extra-4.js",[],"/home/david/proxectos/advanced-react-hooks/src/final/02.js",[],"/home/david/proxectos/advanced-react-hooks/src/final/02.extra-1.js",[],"/home/david/proxectos/advanced-react-hooks/src/final/02.extra-2.js",[],"/home/david/proxectos/advanced-react-hooks/src/final/02.extra-3.js",[],"/home/david/proxectos/advanced-react-hooks/src/final/03.js",[],"/home/david/proxectos/advanced-react-hooks/src/final/03.extra-1.js",[],"/home/david/proxectos/advanced-react-hooks/src/final/03.extra-2.js",[],"/home/david/proxectos/advanced-react-hooks/src/final/05.js",[],"/home/david/proxectos/advanced-react-hooks/src/final/06.js",[],"/hom